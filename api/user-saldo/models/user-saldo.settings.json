    },
    "monto": {
      "type": "decimal"
    },
    "formapago": {
      "type": "string"
    },
    "banco": {
      "type": "richtext"
    },
    "referencia": {
      "type": "string"
    },
    "observacion": {
      "type": "string"
    },
    "fecha": {
      "type": "date"
    },
    "tipo": {
      "type": "integer"
    }
  }
